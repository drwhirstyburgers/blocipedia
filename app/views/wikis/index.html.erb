<div class="row">
  <div class="col-md-4">
  <h1>Wikis</h1>
  <% @wikis.each do |wiki| %>
  <div class="media">
    <div class="media-body">
      <h4 class="media-heading">
        <%= link_to wiki.title, wiki %>
      </h4>
      <small><%= wiki.body %></small>
    </div>
  </div>
  <% end %>
  <% if current_user %>
    <%= link_to "Create Wiki", new_wiki_path, class: 'btn btn-success' %>
  <% end %>
   </div>
</div>
