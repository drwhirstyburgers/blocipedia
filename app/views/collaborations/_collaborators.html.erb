<h4>Collaborators:</h4>
<ul>
  <% @wiki.collaborators.each do |collaborator| %>
  <li>
    <strong><%= User.find_by_id(collaborator.user_id).username %></strong>
    <% if current_user == @wiki.user %>
      <%= link_to "Remove Collaborator", [@wiki, collaborator], class: 'btn btn-primary btn-sm', method: :delete, data: {confirm: 'Are you sure you want to delete this collaborator?'} %>
    <% end %>
  </li>
  <% end %>

</ul>
